{
  "name": "interactive-tree-structure-editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest --testPathIgnorePatterns='.perf.test'",
    "test:all": "jest && playwright test --reporter=list",
    "test:ci": "jest --ci --testPathIgnorePatterns='.perf.test'",
    "test:watch": "jest --watch --testPathIgnorePatterns='.perf.test'",
    "test:coverage": "jest --coverage --testPathIgnorePatterns='.perf.test'",
    "test:e2e": "playwright test",
    "test:e2e:ci": "playwright test --reporter=list",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "cross-env TEST_RUNNER_PATH=./node_modules/.bin/jest reassure --testMatch='**/__tests__/performance/**/*.perf.test.{ts,tsx}'",
    "test:performance:baseline": "cross-env TEST_RUNNER_PATH=./node_modules/.bin/jest reassure --baseline --testMatch='**/__tests__/performance/**/*.perf.test.{ts,tsx}'",
    "test:performance:compare": "reassure compare"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "reassure": "^1.4.0",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.3",
    "vite": "^7.2.2"
  }
}
